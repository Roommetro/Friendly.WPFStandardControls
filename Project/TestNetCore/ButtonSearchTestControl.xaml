<UserControl x:Class="Test.ButtonSearchTestControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Test"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Button x:Name="_button1" Content="Button" HorizontalAlignment="Left" Margin="22,24,0,0" VerticalAlignment="Top" Width="75" Command="ApplicationCommands.Open" CommandParameter="button1"/>
        <Button x:Name="_button2" Content="Button" HorizontalAlignment="Left" Margin="22,64,0,0" VerticalAlignment="Top" Width="75" Command="ApplicationCommands.Close"/>
        <Button x:Name="_button3" Content="Button" HorizontalAlignment="Left" Margin="22,106,0,0" VerticalAlignment="Top" Width="75" Command="ApplicationCommands.Open" CommandParameter="button3"/>
        <Button x:Name="_button4" Content="Button" HorizontalAlignment="Left" Margin="22,143,0,0" VerticalAlignment="Top" Width="75" Command="ApplicationCommands.Open" CommandParameter="{x:Static local:CommandPrameterType.A}" />
        <Button x:Name="_button5" Content="Button" HorizontalAlignment="Left" Margin="22,184,0,0" VerticalAlignment="Top" Width="75" IsCancel="True"/>
    </Grid>
</UserControl>
